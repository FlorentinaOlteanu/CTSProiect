package ro.ase.csie.cts.builder;

public class GiftBuilder {
	
	public Gift crateGiftF (){
	      Gift c = new Gift();
	      c.adaugaProdus(new WhiteChocolate());
	      c.adaugaProdus(new FemalePerfume());
	      return c;
	   }   

	public Gift crateGiftM (){
	      Gift gift = new Gift();
	      gift.adaugaProdus(new DarkChocolate());
	      gift.adaugaProdus(new MalePerfume());
	      return gift;
	   }  
}
