package ro.ase.csie.cts.builder;

public class GiftBuilder {
	
	public Gift creeazaCadouF (){
	      Gift c = new Gift();
	      c.adaugaProdus(new CiocolataAlba());
	      c.adaugaProdus(new ParfumF());
	      return c;
	   }   

	public Gift creeazaCadouB (){
	      Gift gift = new Gift();
	      gift.adaugaProdus(new CiocolataNeagra());
	      gift.adaugaProdus(new ParfumB());
	      return gift;
	   }  
}
