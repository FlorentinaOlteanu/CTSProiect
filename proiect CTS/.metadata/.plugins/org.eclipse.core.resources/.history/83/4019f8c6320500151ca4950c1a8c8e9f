package ro.ase.csie.cts.builder;

public class CadouBuilder {
	
	public Cadou creeazaCadouF (){
	      Cadou c = new Cadou();
	      c.adaugaProdus(new CiocolataAlba());
	      c.adaugaProdus(new ParfumF());
	      return c;
	   }   

	public Cadou creeazaCadouB (){
	      Cadou cadou = new Cadou();
	      cadou.adaugaProdus(new CiocolataNeagra());
	      cadou.adaugaProdus(new ParfumB());
	      return cadou;
	   }  
}
